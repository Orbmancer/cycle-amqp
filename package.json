    "type": "git",
    "url": "git+https://github.com/Orbmancer/cycle-amqp.git"
  },
  "keywords": [
    "cycle",
    "driver",
    "amqp",
    "javascript",
    "stream"
  ],
  "author": "Orbmancer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Orbmancer/cycle-amqp/issues"
  },
  "homepage": "https://github.com/Orbmancer/cycle-amqp#readme",
  "dependencies": {
    "@cycle/xstream-run": "^4.0.0",
    "amqplib": "^0.5.1",
    "xstream": "^9.0.0"
  },
  "devDependencies": {
    "mocha": "^3.2.0"
  }
}
